{
	"version": "0.1.0",
	"command": "npm",
	"isShellCommand": true,
	"showOutput": "always",
	"suppressTaskName": true,
	"tasks": [
		{
			"taskName": "install",
			"args": ["install"]
		},
		{
			"taskName": "start",
			"isBuildCommand": true,
			"args": ["start"]
		},
		{
			"taskName": "test",
			"args": ["run", "test"]
		},
		{
			"taskName": "format",
			"args": ["run", "format"]
		}
	]
}